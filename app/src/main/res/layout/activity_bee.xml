<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="match_parent" tools:context=".BEE">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <ScrollView android:layout_width="match_parent" android:layout_weight="100" android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp" tools:layout_editor_absoluteY="1dp" app:layout_constraintTop_toTopOf="parent">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="vertical">
            <Button android:id="@+id/btn1" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="basic technolgy and their concept" android:layout_marginTop="5dp" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn2" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="36dp" android:text="d.c. circuits" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn3" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:paddingLeft="36dp" android:text="capacitor" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn4" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:paddingLeft="40sp" android:text="battery" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn5" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="electro magnetism" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn6" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="electromagnetic induction" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn7" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="a.c. circuits" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn8" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="the phase circuits" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn9" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="overview to d.c circuits" android:layout_marginTop="5dp" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn10" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="36dp" android:text="d.c. circuit theorem" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn11" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:paddingLeft="36dp" android:text="voltage and current source" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn12" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:paddingLeft="40sp" android:text="electromagnetic induction" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn13" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="battery" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn14" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="ac fundamentals" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
            <Button android:id="@+id/btn15" android:layout_width="match_parent" android:layout_height="85dp" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"  android:paddingLeft="40sp" android:text="various types of power plants" android:fontFamily="@font/choco" android:textSize="25sp" android:textColor="@color/black" app:backgroundTint="#D6D6D6" style="bold"/>
        </LinearLayout>
    </ScrollView>
        <com.google.android.gms.ads.AdView
            android:id="@+id/adView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:layout_gravity="center_horizontal"
            app:adSize="BANNER"
            app:adUnitId="@string/ad_unit_id"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />
</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>